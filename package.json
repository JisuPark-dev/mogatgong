{
  "name": "front-house-call",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite --port 8085",
    "serve-dev": "vite --port 8085 --mode dev --host",
    "serve-stag": "vite --port 8085 --mode stag --host",
    "serve-prod": "vite --port 8085 --mode prod",
    "build": "node prebuild.js && vite build --mode build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build --mode prod",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@sentry/vite-plugin": "^2.16.1",
    "@sentry/vue": "^8.31.0",
    "@tanstack/vue-query": "^5.40.0",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "v-calendar": "^3.1.2",
    "vue": "^3.4.21",
    "vue-inline-svg": "^3.1.2",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.0",
    "bootstrap-vue-next": "^0.9.23",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^1.5.4",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  }
}
